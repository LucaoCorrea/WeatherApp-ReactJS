{
    "env": {
        "browser": true,
        "es2021": true,
        "node": 1

    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "singleQuote": false,
        "linebreak-style" : 0,
        "quotes": [2, "single", { "avoidEscape": true }]
    }

}
